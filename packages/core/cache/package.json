{
  "name": "@absolute-web/vsf-cache",
  "version": "0.0.3",
  "license": "MIT",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./nuxt": "./nuxt/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "@absolute-web/vsf-core": "~0.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*",
    "defaultHandler/**/*",
    "nuxt/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AbsoluteWebServices/vue-storefront.git"
  },
  "bugs": {
    "url": "https://github.com/absolutewebservices/vue-storefront/issues"
  },
  "homepage": "https://github.com/absolutewebservices/vue-storefront#readme"
